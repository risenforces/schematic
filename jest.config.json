{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "src",
  "moduleNameMapper": {
    "^@app$": "<rootDir>",
    "^@app/(.*)$": "<rootDir>/$1"
  },
  "testRegex": ".test.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "../coverage",
  "testEnvironment": "node"
}